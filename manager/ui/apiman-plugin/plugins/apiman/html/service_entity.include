      <div class="apiman-entity-summary" ng-controller="Apiman.ServiceEntityController">
        <div class="row apiman-entity-breadcrumb">
          <div class="col-md-12">
            <div class="title container-fluid">
              <i class="breadcrumb-icon fa fa-puzzle-piece"></i>
              <div class="entity emphasis">
                <a data-field="serviceName" href="{{ pluginName }}/service-overview.html?org={{ service.organization.id }}&service={{ service.id }}" data-role="dummy">{{ service.name }}</a>
              </div>
              <div class="versions">
                <div class="btn-group apiman-entity-action" data-role="dummy" data-field="versions">
                  <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                    Version: {{selectedServiceVersion.version}}
                    <span class="caret"></span>
                  </button>
                  <ul class="dropdown-menu" role="menu">
                    <li ng-repeat="serviceVersion in versions"><a href="#" ng-click="setVersion( serviceVersion )">{{ serviceVersion.version }}</a></li>
                  </ul>
                </div>
                <a apiman-permission="appEdit" href="{{ pluginName }}/new-serviceversion.html?org={{ org.id }}&service={{ service.id}}" class="btn btn-primary apiman-entity-action" data-field="toNewServiceVersion" data-i18n-key="new-version">New Version</a>
              </div>
            </div>
            <hr />
          </div>
        </div>
        <div class="row apiman-entity-metadata">
          <div class="col-md-7" style="margin-bottom: 8px">
            <!-- Service Summary -->
            <div class="description" data-field="description" data-role="dummy">{{ service.description }}</div>
            <div style="padding: 8px">
              <div class="entity-info-with-icon">
                <i class="fa fa-clock-o fa-fw"></i>
                <span class="apiman-label-faded" data-i18n-key="created-on">Created on</span>
                <span data-field="createdOn" data-role="dummy" >{{ service.createdOn | date:'yyyy-MM-dd' }}</span>
              </div>
              <div class="entity-info-with-icon">
                <i class="fa fa-user fa-fw"></i>
                <span class="apiman-label-faded" data-i18n-key="created-by">Created by</span>
                <span><a href="{{ pluginName }}/user-orgs.html?user={{ service.createdBy }}" data-field="createdBy" data-role="dummy">{{ service.createdBy }}</a></span>
              </div>
            </div>
            <div class="entity-info-with-icon">
              <span data-i18n-key="status-label">Status:</span>
              <span class="apiman-label apiman-label-warning" data-field="status" data-role="dummy">{{ selectedServiceVersion.status }}</span>
            </div>
          </div>
          <div class="col-md-5" apiman-permission="svcEdit">
            <div>
              <div apiman-status="Published">
                <a data-i18n-key="ttdo-new-svc-contract" data-field="ttd_toNewContract" href="new-contract.html">Link my Application to this Service (New Contract)</a>
              </div>
              <div>
                <a data-i18n-key="ttdo-new-svc-version" data-field="ttd_toNewServiceVersion" href="new-serviceversion.html">Create a new version of this Service (New Version)</a>
              </div>
            </div>
            <!-- The Publish Action -->
            <div class="apiman-divider-40"></div>
            <div class="">
              <button class="btn btn-primary" data-field="publishButton" data-i18n-key="publish" placeholder="Publishing..." data-icon="fa-cog">Publish</button>
              <button class="btn btn-warning" data-field="retireButton" data-i18n-key="retire" placeholder="Retiring..." data-icon="fa-cog">Retire</button>
            </div>
          </div>
        </div>
      </div>